procedure SendCommand(command: string);
begin
  SendKeys(command, 60, 30);
  PressKey(13);
end;

procedure HomeCommand;
begin
   SendCommand('::home');
end;

procedure EmptyCommand;
begin
   SendCommand('::empty');
end;

