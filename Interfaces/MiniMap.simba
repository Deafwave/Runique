function RuniqueFlagNotPresent:boolean;
begin
  if not findColorSpiral(x, y, 284358, RuniqueMinimap) then result:=  true;
end;


procedure RuniqueWaitFlag;
begin
  waitFunc(@RuniqueFlagNotPresent, 50, 10000);
end;
